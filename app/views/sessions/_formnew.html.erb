
<div class="row">
  <div class="span4">
  <div class="well well-small">
  <%= form_for(:session, url: sessions_path) do |f| %>
    
    <%= collection_select(:user, :name, @users, :name, :name) %>

     <%= f.label :password %>
     <%= f.password_field :password %>

     <%= f.submit "Sign in", class: "btn btn-large btn-success" %>
   <% end %>

   <p><font color="black">Name not in the list? <%= link_to "Sign up here!", new_user_path %></p>

</div>
</div>
</div>
